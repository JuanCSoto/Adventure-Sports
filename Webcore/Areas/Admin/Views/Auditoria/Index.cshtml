@model Webcore.Areas.Admin.Models.Auditoria
@{
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<ul id="MenuP">
    <li class="Menubig">@Model.Module.Name</li>
</ul>
<div class="cont" align="left">
    <hr />
    @Resources.Extend.Messages.INFORMATION_AUDIT
    <p class="p-small">
        <input onclick="window.location.href='@Url.Action("ExportAudit", new { Area = "Admin" })'" type="button" class="buttons" value="@Resources.Global.Messages.EXPORT" />
        @Resources.Extend.Messages.EXPORT_AUDIT
    </p>
    <p class="p-small">
        <input onclick="if(confirm('@Resources.Extend.Messages.CLEAN_AUDIT')) { window.location.href='@Url.Action("CleanAudit", new { Area = "Admin" })'; }" type="button" class="buttons" value="@Resources.Global.Messages.CLEAN" />
        @Resources.Extend.Messages.CLEAN_AUDIT_MESSAGE
    </p>
</div>