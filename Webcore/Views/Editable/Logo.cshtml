@{
  Layout = null;
  string content = Url.Content("~/").TrimEnd('/');
  string logo = Business.Utils.GetFronEndValue(Domain.Entities.Basic.ForntEndEditableType.LOGO);
}

<div id="editable-box" class="alerta">
  <div class="alerta-titulo">
    <h1>@ViewBag.TXTEDITLOGO</h1>
    <div class="alerta-cerrar">
      <img src="~/resources/images/mimedellin/cerrar.png" border="0" alt="icon" />
    </div>
  </div>
  <div class="clear"></div>

  <div class="editable-content">
    <div>
    <img id="editable-image" class="site-logo" src="@content@logo" alt="logo" />
      </div>
    <div class="registro-item">
      <div class="registro-txt">Logo del sitio (464 x 80):</div>
      <div class="seleccionar-imagen">
        <a>+</a>
        <h4>@ViewBag.TXTSELIMG</h4>        
        <input id="editableImage" name="editableImage" type="hidden" class="editable-value" data-id="@((int)Domain.Entities.Basic.ForntEndEditableType.LOGO)" data-type="image" value="@logo"/>
        <iframe id="frame-user-file" src="@(content)/editable/uploadimage" scrolling="no" onload="checkIframeImageLoaded('frame-user-file');"></iframe>
      </div>
    </div>
  </div>
  <div class="clear"></div>
  <div id="error-file" class="error" style="display:none;">@Resources.Extend.Messages.IMAGEMX</div>
  <div class="clear"></div>
    <div class="centrado">
  <div class="alerta-aceptar">
    <a>
      <h3>@ViewBag.SAVE</h3>
      <span></span>
    </a>
  </div>
        </div>
  <div class="clear"></div>
</div>
