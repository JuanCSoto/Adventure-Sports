@{
  Layout = null;
  string content = Url.Content("~/").TrimEnd('/');  
}

<div id="editable-box" class="alerta">
  <div class="alerta-titulo">
    <h1>@ViewBag.TXTTEXTCOM</h1>
    <div class="alerta-cerrar">
      <img src="~/resources/images/mimedellin/cerrar.png" border="0" alt="icon" />
    </div>
  </div>
  <div class="clear"></div>

  <div class="editable-content">
    <div class="registro-item">
      <div class="registro-txt">@ViewBag.TXTTITPART <span>:</span></div>
      <div class="editable-input">
        <input type="text" maxlength="10" class="editable-value registro-input-editable" data-id="@((int)Domain.Entities.Basic.ForntEndEditableType.COMMUNITY_TEXT_1)" data-type="text" value="@Business.Utils.GetFronEndValue(Domain.Entities.Basic.ForntEndEditableType.COMMUNITY_TEXT_1)"/>
      </div>
    </div>
  </div>

  <div class="editable-content">
    <div class="registro-item">
      <div class="registro-txt">@ViewBag.TXTTITPART <span>:</span></div>
      <div class="editable-input">
        <input type="text" maxlength="10" class="editable-value registro-input-editable" data-id="@((int)Domain.Entities.Basic.ForntEndEditableType.COMMUNITY_TEXT_2)" data-type="text" value="@Business.Utils.GetFronEndValue(Domain.Entities.Basic.ForntEndEditableType.COMMUNITY_TEXT_2)"/>
      </div>
    </div>
  </div>


  <div class="editable-content">
    <div class="registro-item">
      <div class="registro-txt">@ViewBag.DESCRIPTION <span>:</span></div>
      <div class="editable-input">
        <input type="text" maxlength="50" class="editable-value registro-input-editable" data-id="@((int)Domain.Entities.Basic.ForntEndEditableType.COMMUNITY_DESCRIPTION)" data-type="text" value="@Business.Utils.GetFronEndValue(Domain.Entities.Basic.ForntEndEditableType.COMMUNITY_DESCRIPTION)"/>
      </div>
    </div>
  </div>

  

  <div class="clear"></div>
  <div class="centrado">
    <div class="alerta-aceptar">
      <a>
        <h3>@ViewBag.SAVE</h3>
        <span></span>
      </a>
    </div>
  </div>
  <div class="clear"></div>
</div>
